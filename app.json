{
  "expo": {
    "name": "my-app",
    "slug": "BiteShare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We would like to find the restaurants near you",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "config":{
        "googleSignIn": {
          "reservedClientId":"com.googleusercontent.apps.90738707092-9ik3l59ad22hfghgrms9ti5cgn2da675"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
